def fib(terms):
    if terms==1 :
        return 0
    elif terms==2 :
        return 1
    else :
        return fib(terms-1)+fib(terms-2)
terms=int(input("enter no.of terms:"))
print("fibonacci series:")
for i in range (1,terms+1) :
            print(fib(i),end=" ")
#without using recursion
terms=int(input("\nenter no.of terms:"))
a,b=0,1
for i in range(terms):
    print(a,end=" ")
    a,b=b,a+b
